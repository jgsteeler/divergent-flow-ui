{
  "name": "divergent-flow-ui",
  "private": true,
  "version": "0.1.5",
  "type": "module",
  "scripts": {
    "dev": "serve -s dist -l 4137",
    "build": "tsc -b && vite build",
    "postbuild": "node scripts/generate-config.mjs",
    "lint": "eslint .",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "docker:up": "docker compose up -d --build",
    "docker:down": "docker compose stop && docker compose rm -f",
    "docker:logs:prod": "docker compose logs -f ui",
    "docker:logs:dev": "docker compose logs -f ui-dev"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.4",
    "axios": "^1.12.2",
    "openapi-typescript-codegen": "^0.29.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "rollup": "3.29.4",
    "serve": "^14.2.5",
    "standard-version": "^9.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "4.5.0"
  }
}
