{
  "name": "divergent-flow-ui",
  "private": true,
  "version": "0.1.9",
  "type": "module",
  "scripts": {
    "dev": "serve -s dist -l 6137",
    "build": "tsc -b && vite build",
    "postbuild": "node scripts/generate-config.mjs",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "docker:up": "docker compose up -d --build",
    "docker:up:prod": "docker compose -f docker-compose.yml up -d --build",
    "docker:up:dev": "docker compose -f docker-compose.dev.yml up -d --build",
    "docker:down": "docker compose down",
    "docker:build": "docker compose build --no-cache",
    "docker:logs:prod": "docker compose logs -f ui",
    "docker:logs:dev": "docker compose logs -f ui-dev"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.4",
    "axios": "^1.12.2",
    "oidc-client-ts": "^3.4.1",
    "openapi-typescript-codegen": "^0.29.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^4.0.4",
    "dotenv": "^17.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.0.1",
    "rollup": "3.29.4",
    "serve": "^14.2.5",
    "standard-version": "^9.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "4.5.0",
    "vitest": "^4.0.4"
  }
}
